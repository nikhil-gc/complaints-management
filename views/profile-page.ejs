        <h2>Complaints History</h2>
        <div class="row" style="width: 100%;">
          <div class="col-md-12">
            <div class="card">
              <div class="card-body">
                <table class="table table-info">
                  <tr>
                    <th class="table-light">complaint Id</th>
                    <th class="table-light">Date</th>
                    <th class="table-light">Agreement Number</th>
                    <!-- <th class="table-light">Phone</th> -->
                    <!-- <th class="table-light">Department</th> -->
                    <th class="table-light">Category</th>
                    <th class="table-light">Status</th>
                  </tr>
                  <% if(Object.keys(arr).length==0){%>
                    <tr>
                      <td colspan="5">No previous Complaints !</td>
                    </tr>
                    <% } else{ %>
                      <% for(i=0;i<Object.keys(arr).length;i++){ %>
                        <tr>
                          <td>
                            <%=arr[i].complaintId%>
                          </td>
                          <td>
                            <%=arr[i].complaintDate.toDateString()%>
                          </td>
                          <td>
                            <%=arr[i].agreementNumber%>
                          </td>
                          <td>
                            <%=arr[i].category%>
                          </td>
                          <% if(arr[i].status=="completed" ){%>
                            <td style="background-color: lightgreen;">
                              <%=arr[i].status%>
                            </td>
                            <% } else{ %>
                              <td style="background-color: lightcoral;">
                                <%=arr[i].status%>
                              </td>
                              <% } %>
                        </tr>
                        <% } %>
                          <% } %>

                </table>
              </div>
            </div>
